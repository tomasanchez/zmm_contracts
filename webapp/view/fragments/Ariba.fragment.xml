<core:FragmentDefinition 
	xmlns="sap.m" 
	xmlns:tnt="sap.tnt" 
	xmlns:l="sap.ui.layout" 
	xmlns:f="sap.ui.layout.form" 
	xmlns:core="sap.ui.core">
	<Dialog title="{i18n>aribaDialogTitle}" id="aribaDialog">
		<l:Grid defaultSpan="L12 M12 S12" width="auto" id="idGrid">
			<l:content>
				<VBox width="100%" direction="Column" id="__vbox1">
					<Table id="table" width="auto" items="{path: '/Material_ListSet', sorter: {path: 'Maktx', descending: false }}" noDataText="{worklistView>/tableNoDataText}" busyIndicatorDelay="{worklistView>/tableBusyDelay}" growing="true" growingScrollToLoad="true" updateFinished=".onUpdateFinished">
						<columns>
							<Column id="nameColumn">
								<Text text="{i18n>tableNameColumnTitle}" id="nameColumnTitle"/>
							</Column>
						</columns>
						<items>
							<ColumnListItem type="Navigation" press=".onPress">
								<cells>
									<ObjectIdentifier title="{Maktx}"/>
								</cells>
							</ColumnListItem>
						</items>
					</Table>
				</VBox>
			</l:content>
		</l:Grid>
		<buttons>
			<Button text="Close" press="onClose" type="Default"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>