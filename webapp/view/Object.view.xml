<mvc:View controllerName="pampa.comunicacionesformales.contratos.controller.Object" 
	xmlns="sap.m" 
	xmlns:tnt="sap.tnt" 
	xmlns:semantic="sap.m.semantic" 
	xmlns:f="sap.f" 
	xmlns:mvc="sap.ui.core.mvc">
	<!-- Framed DynamicPage -->
	<f:DynamicPage id="dynamicPageId" preserveHeaderStateOnScroll="true" busy="{objectView>/busy}" busyIndicatorDelay="{objectView>/delay}" backgroundDesign="List" headerExpanded="{/headerExpanded}" showFooter="false" stickySubheaderProvider="iconTabBar" class="sapUiNoContentPadding">
		<!-- DynamicPage Title -->
		<f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<Title text="{IdContrato}" level="H2"/>
				</f:heading>
				<f:breadcrumbs>
					<Breadcrumbs currentLocationText="{IdContrato}">
						<Link text="{i18n>home}" press=".onNavBack" />
					</Breadcrumbs>
				</f:breadcrumbs>
			</f:DynamicPageTitle>
		</f:title>
		<!-- DynamicPage Header -->
		<f:header >
			<f:DynamicPageHeader pinnable="false">
				<Title text="{IdContrato}" level="H2"/>
			</f:DynamicPageHeader>
		</f:header>
		<!-- DynamicPage Content -->
		<f:content>
			<!-- Tab Filter -->
			<IconTabBar id="iconTabBar" select=".onFilterSelect" class="sapUiResponsiveContentPadding sapUiNoMarginTop" expanded="{device>/isNoPhone}"  visible="false" >
				<items>
					<IconTabFilter icon="sap-icon://employee" iconColor="Default"  text="{i18n>contratos_activos_label}" key="EmpleadosSet"/>
					<IconTabFilter icon="sap-icon://decision" iconColor="Neutral"  text="{i18n>contratos_finalizados_label}" key="F"/>
					<!-- <IconTabFilter icon="sap-icon://inventory" iconColor="Negative" count="{/ProductCollectionStats/Counts/Weight/Overweight}" text="Overweight" key="Overweight" /> -->
				</items>
				<content/>
			</IconTabBar>
		</f:content>
	</f:DynamicPage>
</mvc:View>